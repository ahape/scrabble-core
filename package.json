{
    "name": "scrabble-core",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://github.com/ahape/scabble-core.git",
    "author": "<ahape@brightmetrics.com>",
    "license": "MIT",
    "scripts": {
        "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/*.ts'",
        "cli": "clear && env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ts-node cli/index.ts",
        "build": "tsc -p ./",
        "build-types": "tsc src/scrabblecore.ts --declaration --emitDeclarationOnly --skipLibCheck --outFile types/scrabblecore/index.d.ts",
        "bundle": "npx webpack"
    },
    "dependencies": {
        "knockout": "^3.5.1",
        "underscore": "^1.13.1"
    },
    "devDependencies": {
        "@types/knockout": "^3.4.70",
        "@types/mocha": "^8.2.2",
        "@types/node": "^16.0.0",
        "@types/underscore": "^1.11.3",
        "mocha": "^9.0.2",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.5",
        "webpack": "^5.42.0",
        "webpack-cli": "^4.7.2"
    }
}
